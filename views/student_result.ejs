<%- include('include/_header') %>

<main id ="site-main">
    <div class="container">
        <div class="form-title text-center">
           <h2 class="text-dark">
            Your Result
           </h2>
        </div>
        <div class="new_student">
            <div class="student-result">
                 <p class="text-light">Roll No</p>
                 <p class="rollno">: <%= result.rollno %></p>
            </div>
            <div class="student-result">
                <p class="text-light">Name</p>
                <p class="name">: <%= result.name %></p>
            </div>
            <div class="student-result">
                <p class="text-light">Date of Birth</p> 
                <p class="dob"> : <%= result.dob %></p>
            </div>
            <div class="student-result">
                <p class="text-light">Score</p>
                <p class="score">: <%= result.score %></p>
            </div>
        </div>
     
       
    </div>
</main>

<%- include('include/_footer') %>

